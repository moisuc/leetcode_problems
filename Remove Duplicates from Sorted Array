class Solution {

    /**
     * @param Integer[] $nums
     * @return Integer
     */
  function removeDuplicates(&$nums) {
      $duplicate = $nums;
        for($i=0;$i<count($duplicate);$i++){
            $needle = $duplicate[$i];
            for($j=$i+1;$j<count($duplicate);$j++){
            if($needle!==$duplicate[$j]){
                break;
            }
           unset($nums[$j]);}
        }
        sort($nums);
    }
}
